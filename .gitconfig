[push]
	default = simple
[alias]
	sglog = "log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%ae>%Creset' --graph -10"
[user]
	name = "Jake Moening"
	email = jmoening@unsignedbytes.com
[merge]
	tool = kdiff3
[mergetool]
	keepBackup = false
[mergetool "kdiff3"]
	path = "C:/Program Files/KDiff3/kdiff3.exe"
	trustExitCode = false
[diff]
	tool = kdiff3
[difftool]
	prompt = false
[difftool "kdiff3"]
	path = "C:/Program Files/KDiff3/kdiff3.exe"
	trustExitCode = false
[core]
	editor = gvim
